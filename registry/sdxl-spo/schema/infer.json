{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SDXL SPO Inference Parameters",
  "type": "object",
  "properties": {
    "prompt": { 
      "type": "string",
      "description": "Image prompt. SPO model optimized for aesthetics."
    },
    "negative_prompt": { "type": "string" },
    "width": { "type": "integer", "minimum": 64 },
    "height": { "type": "integer", "minimum": 64 },
    "steps": { 
      "type": "integer", 
      "minimum": 1,
      "description": "Recommended: 25-50 steps"
    },
    "guidance_scale": { 
      "type": "number", 
      "minimum": 0,
      "description": "Recommended: 5.0"
    },
    "seed": { "type": "integer", "minimum": 0 }
  },
  "required": ["prompt"],
  "additionalProperties": false
}

